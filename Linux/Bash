#-> cat > 1.sh
echo "Hi"
#-> cat 1.sh
echo "Hi"
#-> ll
total 56
-rw-r--r-- 1 dwh   seacct   10 Jun 21 16:12 1.sh
#-> sh 1.sh
Hi
#-> ./1.sh
bash: ./1.sh: Permission denied
#-> . ./1.sh
Hi


